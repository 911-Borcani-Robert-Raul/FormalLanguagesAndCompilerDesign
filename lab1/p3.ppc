fun sum_n_numbers: numbers
    sum = 0
    foreach number in numbers repeat
        sum ++ number

fun max_min_numbers: numbers
    minimum = numbers.first::
    maximum = numbers.first::
    foreach number in numbers repeat
        if number > maximum then
            maximum = number
        if number < minimum then
            minimum = number
    
    result = ;minimum;maximum;

